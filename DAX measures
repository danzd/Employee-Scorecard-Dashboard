% of the team utilization = 

VAR AA_AVERAGE = AVERAGEX(
	KEEPFILTERS(VALUES(AWS[Login])),
	CALCULATE(AVERAGE(AWS[Associate Utilization]))
)

VAR TEAM_AVERAGE = CALCULATE(AVERAGE(AWS[Associate Utilization]),
ALLSELECTED(AWS[Login]),
    ALL(AWS[Login])
)
VAR COMPARISON = AA_AVERAGE - TEAM_AVERAGE
VAR PERCENTAGE = DIVIDE(COMPARISON, 1)
RETURN PERCENTAGE
